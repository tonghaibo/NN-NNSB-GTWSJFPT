<ROW num="0" ><GRDJSDS_VALUE >var getType = 0;

//数据加载完毕
function _thisOnpost_loaddata(sheet)
{
	_this.SetToBoolBox(sheet,3,2);
	_this.SetToBoolBox(sheet,6,2);
	
	//接收传入参数
	var filter = &amp;quot;&amp;quot;;
	try {
		var objParam = window.dialogArguments;
		if (objParam.dwbh != &amp;quot;&amp;quot;) {
			_this.SetCellText(sheet,4,3,objParam.dwbh+&amp;quot;:&amp;quot;+objParam.dwmc);
		}
		_this.SetCellText(sheet,7,3,objParam.yyyy+&amp;quot;年&amp;quot;+objParam.mm+&amp;quot;月&amp;quot;);
	} catch (e) {}

	
}

//点击按钮
function _thisOnButtonCtrlClick(id,sheet,row,col)
{
	window.returnValue = getType;
	if (id == &amp;quot;确定&amp;quot;) {
		window.returnValue = getType;		
	}
	else if (id == &amp;quot;取消&amp;quot;) {
		window.returnValue = &amp;quot;&amp;quot;;
	}
	window.close();
}

//鼠标左键点击
function _thisOnMouseLClick(sheet,row,col)
{
	if (row == 3 &amp;&amp; col == 2) {
		getType = 1;
		_this.SetCellText(sheet,6,2,&amp;quot;0&amp;quot;);
	}
	else if (row == 6 &amp;&amp; col == 2) {
		getType = 2;
		_this.SetCellText(sheet,3,2,&amp;quot;0&amp;quot;);
	}
}
