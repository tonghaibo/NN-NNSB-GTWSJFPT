<ROW num="0" ><GRDJSDS_VALUE >var sheet_mes = 0 ;

var v_str

//数据加载完毕
function _thisOnpost_loaddata(sheet)
{
	initLayout()
//	var mesStr = &amp;quot;请选择：\n是：从启动年开始重算 \n否：从当前年开始重算 \n取消：取消重算&amp;quot;;
	v_str = mesStr;	
	loadMes(v_str);
	_this.ShowToolBar(0);
	_this.AutoFixScreenWidth();
}

//布局
function initLayout(){
	_this.SplitSheet(sheet_mes,&amp;quot;H&amp;quot;,&amp;quot;100%&amp;quot;)
}

//接收参数显示
function loadMes(str){
	_this.SetCellText(sheet_mes,1,0,str);
}


//鼠标左键点击
function _thisOnMouseLClick(sheet,row,col)
{		
	if(sheet == sheet_mes){
		var retVal ;		
		if(row == 2 &amp;&amp; col == 0){
		  	retVal = &amp;apos;1&amp;apos;;
		}else if(row == 2 &amp;&amp; col == 1){
		  	retVal = &amp;apos;0&amp;apos;;
		}else if(row == 2 &amp;&amp; col == 2){
		  	retVal = &amp;apos;-1&amp;apos;;
		}	
		if(retVal != &amp;quot;&amp;quot; &amp;&amp; retVal != &amp;quot;undefined&amp;quot; &amp;&amp; retVal != null) {		
			window.returnValue = retVal ;
			window.close();	
		}
	}	
}
