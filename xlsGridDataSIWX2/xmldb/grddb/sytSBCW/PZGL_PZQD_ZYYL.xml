<ROW num="0" ><ID >MAIN</ID><NAME ></NAME><DATDSC >select zy,
    sum(decode(lxbh,&amp;apos;11&amp;apos;, decode(mx_jefx,&amp;apos;借&amp;apos;,nvl(mx_je,0),-1 * nvl(mx_je,0)),0)) ylsr,
    sum(decode(lxbh,&amp;apos;61&amp;apos;, decode(mx_jefx,&amp;apos;借&amp;apos;,nvl(mx_je,0),-1 * nvl(mx_je,0)),&amp;apos;71&amp;apos;, decode(mx_jefx,&amp;apos;借&amp;apos;,nvl(mx_je,0),-1 * nvl(mx_je,0)),0)) bcsr,
    sum(decode(lxbh,&amp;apos;87&amp;apos;, decode(mx_jefx,&amp;apos;借&amp;apos;,nvl(mx_je,0),-1 * nvl(mx_je,0)),0)) zck,/*暂存款*/
    sum(decode(lxbh,&amp;apos;83&amp;apos;, decode(mx_jefx,&amp;apos;借&amp;apos;,nvl(mx_je,0),-1 * nvl(mx_je,0)),0)) zysr,/*转移收入*/
    &amp;apos;&amp;apos; srsum,
    sum(decode(lxbh,&amp;apos;12&amp;apos;, decode(mx_jefx,&amp;apos;借&amp;apos;,-1 * nvl(mx_je,0), nvl(mx_je,0)),0)) ylzc,
    sum(decode(lxbh,&amp;apos;62&amp;apos;, decode(mx_jefx,&amp;apos;借&amp;apos;,-1 * nvl(mx_je,0), nvl(mx_je,0)),&amp;apos;72&amp;apos;, decode(mx_jefx,&amp;apos;借&amp;apos;,-1 * nvl(mx_je,0), nvl(mx_je,0)),0)) bczc,
    sum(decode(lxbh,&amp;apos;97&amp;apos;, decode(mx_jefx,&amp;apos;借&amp;apos;,-1 * nvl(mx_je,0), nvl(mx_je,0)),0)) zfk,/*暂付款*/
    sum(decode(lxbh,&amp;apos;93&amp;apos;, decode(mx_jefx,&amp;apos;借&amp;apos;,-1 * nvl(mx_je,0), nvl(mx_je,0)),0)) zyzc,/*转移支出*/
    &amp;apos;&amp;apos; zcsum
from cw_rjzb_view a                 
where a.sbh = &amp;apos;[%SBH]&amp;apos; and a.zth = &amp;apos;[%ZTH]&amp;apos;
    and a.dwbh is not null 
    and a.yy=[%YY] and a.mm=[%MM] and (a.pzh=&amp;apos;[%PZH]&amp;apos; or a.pzh=&amp;apos;[%CZXH]&amp;apos;)
group by a.sbh,a.zth,a.yy,a.mm,a.dwbh,a.zy,dd,sxh,lsh
order by sxh,lsh</DATDSC><C4 >MAIN</C4><C5 >MAIN</C5></ROW>
</ROWSET>
