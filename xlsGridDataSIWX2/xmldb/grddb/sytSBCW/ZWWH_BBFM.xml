	objParam.crtusr = G_USRNAM;	//制表人
	objParam.crtdat = G_LOGDAT;	//制表时间

}

//封面设置
function fmSetup()
{
	
	var retval = window.showModalDialog(&amp;quot;show.sp?grdid=ZWWH_BBFMSETUP&amp;quot;,objParam,&amp;quot;dialogWidth=600px;dialogHeight=400px&amp;quot;);
	if (retval != &amp;quot;&amp;quot; &amp;&amp; retval != &amp;quot;undefined&amp;quot; &amp;&amp; retval != undefined) {
		objParam = retval;
		
		_this.SetCellText(0,1,2,objParam.title);
		_this.SetCellText(0,2,2,objParam.title2);
		_this.SetCellText(0,4,3,objParam.bzdw);
		_this.SetCellText(0,10,3,objParam.bbyy+&amp;quot;年&amp;quot;+objParam.bbmm+&amp;quot;月&amp;quot;);
		_this.SetCellText(0,12,3,objParam.crtdat);
	
	}
	
}

//打印
function printFM()
{
	_this.Print(0,0);
}
