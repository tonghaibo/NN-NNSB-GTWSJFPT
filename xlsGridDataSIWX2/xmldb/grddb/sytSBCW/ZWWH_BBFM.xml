{
	objParam.bbname = &amp;quot;&amp;quot;;		//报表名称
	objParam.title = &amp;quot;&amp;quot;;		//标题
	objParam.title2 = &amp;quot;&amp;quot;;		//副标题
	objParam.bzdw = &amp;quot;&amp;quot;;		//编制单位
