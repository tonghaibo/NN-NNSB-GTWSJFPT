<ROW num="0" ><ID >MAIN</ID><NAME ></NAME><DATDSC >select * from
(
select distinct row_number() over (order by crtdat desc) rn,
	crtdat,reference,businesscode,senderinstid,recverinstid,
       transactiondate,transactiontime,
       bkseq,grbh,grxm,je,ssq,(100*jfdc)||&amp;apos;%&amp;apos; jfdc,
       (select aaa103 from aa10 where aaa100 = &amp;apos;AAE140&amp;apos; and YWXT_DRDM= a.xzlx) xzlx,
       ywdjh,skzh,skhm,fkzh,fkhm
from si_msgtextlog a  
where nvl(senderinstid,&amp;apos;%&amp;apos;) like DECODE(&amp;apos;[%SENDER]&amp;apos;,0,&amp;apos;%&amp;apos;, &amp;apos;[%SENDER]%&amp;apos;)
  and nvl(bkseq,&amp;apos;%&amp;apos;) like &amp;apos;[%yhlsh]%&amp;apos;
  and to_char(crtdat,&amp;apos;yyyy-mm-dd&amp;apos;)&amp;gt;=&amp;apos;[%DAT1]&amp;apos;
  and to_char(crtdat,&amp;apos;yyyy-mm-dd&amp;apos;)&amp;lt;=&amp;apos;[%DAT2]&amp;apos;
  and reference like &amp;apos;%[%Ref]%&amp;apos;
  and nvl(recverinstid,&amp;apos;%&amp;apos;) like decode(&amp;apos;[%recvinstid]&amp;apos;,0,&amp;apos;%&amp;apos;,&amp;apos;[%recvinstid]%&amp;apos;)
  and ywdjh is not null
  and grbh like &amp;apos;%[%GRBH]%&amp;apos;
  and (100*jfdc) like &amp;apos;[%JFDC]%&amp;apos;
)  
order by crtdat desc,reference</DATDSC><C4 >MAIN</C4><C5 >MAIN</C5><C6 >MAIN</C6><C7 >MAIN</C7><C8 >MAIN</C8><C9 >MAIN</C9><C10 >MAIN</C10><C11 >MAIN</C11><C12 >MAIN</C12><C13 >MAIN</C13><C14 >MAIN</C14><C15 >MAIN</C15><C16 >MAIN</C16><C17 >MAIN</C17><C18 >MAIN</C18><C19 >MAIN</C19><C20 >MAIN</C20><C21 >MAIN</C21><C22 >MAIN</C22><C23 >MAIN</C23><C24 >MAIN</C24><C25 >MAIN</C25><C26 >MAIN</C26><C27 >MAIN</C27><C28 >MAIN</C28><C29 >MAIN</C29></ROW>
