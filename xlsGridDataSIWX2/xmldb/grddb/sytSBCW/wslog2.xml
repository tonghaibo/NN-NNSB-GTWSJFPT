//数据加载完毕
function _thisOnpost_loaddata(sheet)
{
	_this.AutoFixScreenWidth();
	_this.SetAttribe(&amp;quot;SHEET_0&amp;quot;,_this.ATTR_SHEET_SELECTFLAG,_this. SELECTFLAG_SINGLEROW );
	
	for (var r=_this.GetMainCellRangeRow1(sheet);r&amp;lt;=_this.GetMainCellRangeRow2(sheet);r++) {
		_this.SetCellShowText(sheet,r,_this.GetMainCellRangeCol2(sheet)-1,&amp;quot;双击查看&amp;quot;);
		_this.SetCellShowText(sheet,r,_this.GetMainCellRangeCol2(sheet),&amp;quot;双击查看&amp;quot;);
	}
	
	_this.AutoFixColWidth(sheet,600);
}

//鼠标双击
function _thisOnMouseDClick(sheet,row,col)
{
	var req_txtCol = _this.GetMainCellRangeCol2(0)-1;
	var rsp_txtCol = _this.GetMainCellRangeCol2(0);
	if (row &amp;gt;= _this.GetMainCellRangeRow1(sheet) &amp;&amp; row &amp;lt;= _this.GetMainCellRangeRow2(sheet)) {
		if(col == req_txtCol || col == rsp_txtCol){
			var guid = _this.GetCellText(sheet,row,col);
			window.showModalDialog(&amp;quot;show.sp?grdid=ShowWSLog&amp;guid=&amp;quot;+guid,null,&amp;quot;dialogWidth=600px;dialogHeight=500px&amp;quot;);
		}
		else {
			TaskLog();
		}	
	}
}

//查询后台日志
function TaskLog()
{
	var curr_row = _this.GetCurrentRow(0);
	var ref = &amp;quot;&amp;quot;;
	if(curr_row != -1) {
		ref = _this.GetCellText(0,curr_row,2);
	}
	window.showModalDialog(&amp;quot;show.sp?grdid=YSZL_TASKLOG&amp;in_ref=&amp;quot;+ref,null,&amp;quot;dialogWidth=1010px;dialogHeight=300px&amp;quot;);
}
